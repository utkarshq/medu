{
  "description": "This component is based on the `button` element and supports all of its props",
  "methods": [],
  "displayName": "Copy",
  "props": {
    "content": {
      "required": true,
      "tsType": {
        "name": "string"
      },
      "description": "The content to copy."
    },
    "variant": {
      "required": false,
      "tsType": {
        "name": "union",
        "raw": "\"mini\" | \"default\" | null",
        "elements": [
          {
            "name": "literal",
            "value": "\"mini\""
          },
          {
            "name": "literal",
            "value": "\"default\""
          },
          {
            "name": "null"
          }
        ]
      },
      "description": "The variant of the copy button.",
      "defaultValue": {
        "value": "\"default\"",
        "computed": false
      }
    },
    "asChild": {
      "required": false,
      "tsType": {
        "name": "boolean"
      },
      "description": "Whether to remove the wrapper `button` element and use the\npassed child element instead.",
      "defaultValue": {
        "value": "false",
        "computed": false
      }
    }
  }
}