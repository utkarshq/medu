{
  "name": "docs-ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:js:cjs && yarn build:js:esm",
    "build:js:cjs": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:js:esm": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "clean": "rimraf dist",
    "dev": "yarn build:js:cjs && yarn build:js:esm",
    "lint": "eslint src --fix"
  },
  "devDependencies": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react@rc",
    "clsx": "^2.0.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.13.0",
    "next": "15.0.4",
    "rimraf": "^5.0.1",
    "tailwind": "*",
    "tailwindcss": "^3.3.3",
    "tsc-alias": "^1.8.7",
    "tsup": "^5.10.1",
    "types": "*",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "next": "*",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@medusajs/icons": "^2.0.0",
    "@medusajs/ui": "^4.0.0",
    "@next/third-parties": "15.0.4",
    "@octokit/request": "^8.1.1",
    "@react-hook/resize-observer": "^1.2.6",
    "@segment/analytics-next": "^1.75.0",
    "@uidotdev/usehooks": "^2.4.1",
    "algoliasearch": "^5.2.1",
    "framer-motion": "^11.11.9",
    "mermaid": "^10.9.0",
    "npm-to-yarn": "^2.1.0",
    "prism-react-renderer": "2.4.0",
    "react": "rc",
    "react-dom": "rc",
    "react-instantsearch": "^7.13.6",
    "react-markdown": "^9.0.1",
    "react-medium-image-zoom": "^5.2.10",
    "react-tooltip": "^5.28.0",
    "react-transition-group": "^4.4.5",
    "react-uuid": "^2.0.0",
    "slugify": "^1.6.6"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
}
